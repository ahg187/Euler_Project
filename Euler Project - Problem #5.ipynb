   "source": [
    "def lcm(a,b):\n",
    "    gcd, tmp = a,b\n",
    "    while tmp != 0:\n",
    "        gcd,tmp = tmp, gcd % tmp\n",
    "    return a*b/gcd"
   ]
  },
